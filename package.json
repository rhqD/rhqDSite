{
  "name": "playground",
  "version": "1.0.0",
  "description": "react+redux+react-router+postcss+babel+webpack+mocha+istanbul",
  "scripts": {
    "dev": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.dev.config.js --progress --colors --host 0.0.0.0",
    "start": "webpack-dev-server --config webpack.dev.config.js --progress --colors --host 0.0.0.0",
    "build": "npm run clearBuild && webpack --config webpack.dev.config.js --progress --profile --colors",
    "clearBuild": "IF EXIST build del \"build\\*.*\" /Q",
    "dist": "npm run clearProd && webpack -p --config webpack.dist.config.js --progress --colors",
    "eslint": "eslint app --ext .js",
    "eslintFix": "eslint app --ext .js --fix",
    "stylelint": "stylelint app/**/*.css",
    "test": "mocha \"./test/**/*.test.js\" --require babel-register --require ./test/setup.js",
    "testWatch": "npm run test -- --reporter min --watch",
    "li": "npm run eslint && npm run stylelint && npm run test",
    "testWithCoverage": "istanbul cover node_modules/mocha/bin/_mocha --require ./test/setup.js -- --compilers jsx:babel-core/register --recursive \"./test/**/*.test.js\" --reporter mocha-teamcity-reporter"
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.20.0",
    "babel-plugin-root-import": "^5.1.0",
    "babel-polyfill": "^6.3.14",
    "classnames": "^2.2.5",
    "isomorphic-fetch": "^2.2.0",
    "lodash": "^4.17.4",
    "react": "^15.0.1",
    "react-autobind": "^1.0.6",
    "react-autocomplete": "^1.5.3",
    "react-bootstrap": "^0.30.9",
    "react-dom": "^15.0.1",
    "react-redux": "^4.2.1",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.0-rc.1",
    "redux": "^3.2.1",
    "redux-thunk": "^1.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "deep-freeze": "0.0.1",
    "eslint": "^2.9.0",
    "eslint-plugin-react": "^5.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.16.0",
    "mocha": "^3.1.2",
    "postcss": "^5.0.21",
    "postcss-assets": "^4.1.0",
    "postcss-browser-reporter": "^0.4.0",
    "postcss-cssnext": "^2.5.2",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "postcss-reporter": "^1.3.3",
    "postcss-url": "^5.1.2",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^3.0.0-beta.5",
    "redux-devtools": "^3.1.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.3",
    "redux-logger": "^2.7.4",
    "style-loader": "^0.13.1",
    "stylelint": "^6.3.3",
    "stylelint-config-standard": "^9.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.9.11",
    "webpack-dev-server": "^1.14.0"
  }
}
