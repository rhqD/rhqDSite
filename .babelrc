{
  "presets": ["es2015", "stage-0", "stage-1", "stage-2", "react"],
  "plugins": [
    "react-hot-loader/babel",
    ["babel-plugin-root-import", [{
      "rootPathPrefix": "~",
      "rootPathSuffix": "app"
    }, {
      "rootPathPrefix": "@",
      "rootPathSuffix": "app/assets"
    }, {
      "rootPathPrefix": "$",
      "rootPathSuffix": "app/components"
    }]]
  ]
}
